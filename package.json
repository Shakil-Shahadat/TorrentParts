{
  "name": "torrentparts",
  "version": "0.0.1",
  "description": "ðŸ“‘ A website to inspect and edit Torrent files and Magnet URLs",
  "main": "bundle.js",
  "dependencies": {
    "browserify": "latest",
    "Buffer": "latest",
    "bytes": "latest",
    "clipboard": "latest",
    "dropzone": "latest",
    "mime-types": "latest",
    "parse-torrent": "latest",
    "tippy.js": "latest",
    "webtorrent": "latest"
  },
  "devDependencies": {
    "terser": "latest",
    "watchify": "latest"
  },
  "scripts": {
    "watch": "watchify parse.js -o bundle.js",
    "compile": "browserify parse.js -o bundle.js",
    "minify": "terser bundle.js -c -m -o bundle.min.js",
    "build": "npm run compile && npm run minify"
  },
